{
  "name": "iot_demo",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon",
    "clean": "npm cache clean --force && rm -rf node_modules package-lock.json && npm install && npm rebuild",
    "start_dev": "pm2 start ecosystem.config.cjs --env development",
    "start_prod": "pm2 start ecosystem.config.cjs --env production",
    "typeorm:sync": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync -d ./src/config/db/typeOrm.ts",
    "seed": "ts-node src/seeders/postgre/globalSeed.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate src/migrations/postgre/PostgreMigration -d src/config/db/typeOrm.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/config/db/typeOrm.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/config/db/typeOrm.ts"
  },
  "_moduleAliases": {
    "@models": "dist/models",
    "@pgmodels": "dist/models/postgre",
    "@mongomodels": "dist/models/mongo",
    "@config": "dist/config",
    "@controllers": "dist/controllers",
    "@routes": "dist/routes",
    "@middlewares": "dist/middlewares",
    "@utils": "dist/utils",
    "@services": "dist/services",
    "@errors": "dist/errors",
    "@seeders": "dist/seeders",
    "@postgrerepositories": "dist/repositories/postgre",
    "@mongorepositories": "dist/repositories/mongo",
    "@dto": "dist/dto",
    "@request": "dist/request",
    "@customTypes": "dist/types"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.3",
    "morgan": "^1.10.0",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/express-validator": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.14.9",
    "@types/pg": "^8.11.6",
    "@types/validator": "^13.12.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^8.57.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2"
  }
}
