{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "plugins": ["@typescript-eslint"],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"]
    },"overrides": [
    {
      "files": ["src/migrations/**/*.ts"],
      "rules": {
        "quotes": ["off"],
        "semi": ["off"]
      }
    }
  ]
  }
  